@page
@model CosmosPagination.Web.Pages.DataModel

@{
    ViewData["Title"] = "Data viewer";
}

<div>
    <form asp-page-handler="seed" method="post">
        <button class="btn btn-primary">Seed data</button>
    </form>
    <form asp-page-handler="delete" method="post">
        <button class="btn btn-danger">Delete data</button>
    </form>
</div>

<div class="alert alert-success">
    @Model.Message
</div>
